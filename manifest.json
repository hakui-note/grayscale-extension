{
    "manifest_version": 3,
    "name": "Grayscale Filter",
    "version": "1.0",
    "permissions": ["tabs"],
    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*", "*://twitter.com/*"],
        "js": ["grayscale.js"]
      }
    ]
  }
  